    "@types/jest": "^23.3.10",
    "@types/pify": "^3.0.2",
    "@types/react": "^16.7.9",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/webdriverio": "^4.13.0",
    "electron": "3.0.10",
    "electron-builder": "^20.36.2",
    "electron-webpack": "^2.6.1",
    "electron-webpack-ts": "^3.1.0",
    "jest": "^23.6.0",
    "spectron": "^5.0.0",
