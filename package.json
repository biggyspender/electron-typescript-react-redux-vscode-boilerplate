    "history": "^4.7.2",
    "pify": "^4.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-localstorage": "^1.0.0-rc5",
    "redux-observable": "^1.0.0",
    "redux-saga": "^0.16.2",
    "rxjs": "^6.3.3",
